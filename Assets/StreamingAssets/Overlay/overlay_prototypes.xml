<?xml version="1.0" encoding="utf-8"?>
<Prototypes>
  <!-- List of overlay prototypes -->
  <!-- id attribute is mandatory and must be unique -->
  <!-- min: minimal value attained by funtion (data will be clipped) -->
  <!-- max: maximal value attained by funtion (data will be clipped) -->
  <!-- color_map: selection of coloring scheme for overlay (defaults to "jet") -->
  <!-- content: lua function name, called on each tile, must return int -->
  <Overlay id="oxygen" min="0" max="1" color_map="Jet">oxygenValueAt</Overlay>
  <Overlay id="room" min="0" max="1" color_map="Random">roomNumberValueAt</Overlay>
  <Overlay id="power" min="0" max="1" color_map="Jet">powerValueAt</Overlay>
  <Overlay id="temperature" min="0" max="1" color_map="Jet">temperatureValueAt</Overlay>
  <Overlay id="debug_thermal_diffusivity" min="0" max="1" color_map="Jet">thermalDiffusivityValueAt</Overlay>
  <Overlay id="debug_heat_generation" min="0" max="1" color_map="Jet">heatGenerationValueAt</Overlay>
</Prototypes>